{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "app-one",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "app-one": {
      "projectType": "application",
      "root": "apps/app-one",
      "sourceRoot": "apps/app-one/src",
      "prefix": "aaron",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/apps/app-one",
            "index": "apps/app-one/src/index.html",
            "main": "apps/app-one/src/main.ts",
            "polyfills": "apps/app-one/src/polyfills.ts",
            "tsConfig": "apps/app-one/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/app-one/src/favicon.ico",
              "apps/app-one/src/assets"
            ],
            "styles": ["apps/app-one/src/styles.scss"],
            "scripts": [],
            "extraWebpackConfig": "apps/app-one/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/app-one/src/environments/environment.ts",
                  "with": "apps/app-one/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "apps/app-one/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "app-one:build:production",
              "extraWebpackConfig": "apps/app-one/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "app-one:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "extraWebpackConfig": "apps/app-one/webpack.config.js",
            "port": 5001
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-one:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/app-one/src/**/*.ts",
              "apps/app-one/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "outputs": ["coverage/apps/app-one"],
          "options": {
            "jestConfig": "apps/app-one/jest.config.js",
            "passWithNoTests": true,
            "extraWebpackConfig": "apps/app-one/webpack.config.js"
          }
        }
      }
    },
    "app-one-e2e": {
      "root": "apps/app-one-e2e",
      "sourceRoot": "apps/app-one-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app-one-e2e/cypress.json",
            "tsConfig": "apps/app-one-e2e/tsconfig.e2e.json",
            "devServerTarget": "app-one:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-one:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/app-one-e2e/**/*.{js,ts}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      }
    },
    "shell": {
      "projectType": "application",
      "root": "apps/shell",
      "sourceRoot": "apps/shell/src",
      "prefix": "aaron",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/apps/shell",
            "index": "apps/shell/src/index.html",
            "main": "apps/shell/src/main.ts",
            "polyfills": "apps/shell/src/polyfills.ts",
            "tsConfig": "apps/shell/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/shell/src/favicon.ico", "apps/shell/src/assets"],
            "styles": ["apps/shell/src/styles.scss"],
            "scripts": [],
            "extraWebpackConfig": "apps/shell/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/shell/src/environments/environment.ts",
                  "with": "apps/shell/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "apps/shell/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "shell:build:production",
              "extraWebpackConfig": "apps/shell/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "shell:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "extraWebpackConfig": "apps/shell/webpack.config.js",
            "port": 5000
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shell:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/shell/src/**/*.ts",
              "apps/shell/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "outputs": ["coverage/apps/shell"],
          "options": {
            "jestConfig": "apps/shell/jest.config.js",
            "passWithNoTests": true,
            "extraWebpackConfig": "apps/shell/webpack.config.js"
          }
        }
      }
    },
    "shell-e2e": {
      "root": "apps/shell-e2e",
      "sourceRoot": "apps/shell-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/shell-e2e/cypress.json",
            "tsConfig": "apps/shell-e2e/tsconfig.e2e.json",
            "devServerTarget": "shell:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "shell:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/shell-e2e/**/*.{js,ts}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      }
    }
  }
}
