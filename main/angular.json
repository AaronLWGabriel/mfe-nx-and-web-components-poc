{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "shell",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "loan": {
      "projectType": "application",
      "root": "apps/loan",
      "sourceRoot": "apps/loan/src",
      "prefix": "mode-loan",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:webpack-browser",
          "options": {
            "outputPath": "dist/apps/loan",
            "index": "apps/loan/src/index.html",
            "main": "apps/loan/src/main.ts",
            "polyfills": "apps/loan/src/polyfills.ts",
            "tsConfig": "apps/loan/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/loan/src/favicon.ico", "apps/loan/src/assets"],
            "styles": ["apps/loan/src/styles.scss"],
            "scripts": [],
            "commonChunk": false,
            "customWebpackConfig": {
              "path": "apps/loan/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/loan/src/environments/environment.ts",
                  "with": "apps/loan/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "apps/loan/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "loan:build:production"
            },
            "development": {
              "browserTarget": "loan:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 5001
          }
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "loan:build",
            "extraWebpackConfig": "apps/loan/webpack.config.js"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/loan/src/**/*.ts",
              "apps/loan/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "outputs": ["coverage/apps/loan"],
          "options": {
            "jestConfig": "apps/loan/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "loan-e2e": {
      "root": "apps/loan-e2e",
      "sourceRoot": "apps/loan-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/loan-e2e/cypress.json",
            "tsConfig": "apps/loan-e2e/tsconfig.e2e.json",
            "devServerTarget": "loan:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "loan:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/loan-e2e/**/*.{js,ts}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      }
    },
    "shell": {
      "projectType": "application",
      "root": "apps/shell",
      "sourceRoot": "apps/shell/src",
      "prefix": "mode",
      "architect": {
        "build": {
